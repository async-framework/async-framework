{
  "deno.enable": true,
  "deno.lint": true,
  "deno.enablePaths": [
    "./packages/examples",
    "./packages/example-apps",
    "./packages/async-loader",
    "./packages/custom-signals",
    "./packages/dev"
  ],
  "editor.formatOnSave": true,
  "deno.config": "./deno.jsonc",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "files.exclude": {
    ".gitignore": true,
    "LICENSE": true,
    ".github/codeql": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "deno.lock": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "deno.lock": true
  }
}
